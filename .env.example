# Copy to .env and fill in with your values

# Database
DATABASE_URL="postgresql+psycopg://<user>:<pass>@<host>/<db>?sslmode=require"
TABLE="demo_chunks"

# Embeddings
EMBED_MODEL="all-MiniLM-L6-v2"
EMBED_DIM="384"
TOP_K="5"
DEFAULT_QUERY="What sentences talk about animals?"

# LLM (Ollama)
OLLAMA_MODEL="gemma3"

# Answer behavior
CONTEXT_THRESHOLD="0.75"

# Generation controls
RESPONSE_TEMPERATURE="0.2"
NUM_CTX="5"
NUM_PREDICT="100"